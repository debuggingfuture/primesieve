{
  "machine": {
    "gpu": 0,
    "cpu": 1000,
    "ram": 100
  },
  "job": {
    "APIVersion": "V1beta1",
    "Spec": {
      "Deal": {
        "Concurrency": 1
      },
      "Docker": {
        "Entrypoint": [
          "./local/bin/primesieve",
          "-q",
          {{if .N}}{{.N}}{{else}}"Find the nth prime -i N=1e10"{{end}}
        ],
        "Image": "ghcr.io/kimwalisch/primesieve/sage-docker-ubuntu-focal-standard-with-targets:84f02afa5c-dirty"
      },  
      "Engine": "Docker",
      "Network": {
        "Type": "None"
      },
      "PublisherSpec": {
        "Type": "IPFS"
      },
      "Resources": {
        "GPU": ""
      },
      "Timeout": 1800,
      "Verifier": "Noop"
    }
  }
}